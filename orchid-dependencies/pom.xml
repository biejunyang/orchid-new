<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <groupId>com.orchid</groupId>
    <artifactId>orchid-dependencies</artifactId>
    <version>1.0-SNAPSHOT</version>

    <modelVersion>4.0.0</modelVersion>
    <packaging>pom</packaging>

    <name>${project.artifactId}</name>
    <description>三方库版本管理</description>

    <properties>
        <!-- java version, encoding-->
        <java.version>1.8</java.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

        <!-- Maven Plugin Versions -->
        <maven.compiler.source>${java.version}</maven.compiler.source>
        <maven.compiler.target>${java.version}</maven.compiler.target>
        <resource.delimiter>@</resource.delimiter>


        <!-- Dependency Versions -->
        <spring-boot.version>2.1.6.RELEASE</spring-boot.version>
        <spring-cloud.version>Greenwich.RELEASE</spring-cloud.version>
        <!--<spring-security-oauth2.version>RELEASE</spring-security-oauth2.version>-->
        <mysql-version>8.0.15</mysql-version>

        <spring-boot-admin.version>2.1.3</spring-boot-admin.version>
        <spring-statemachine.version>2.0.1.RELEASE</spring-statemachine.version>
        <vaadin.version>13.0.1</vaadin.version>
        <okta.version>1.1.0</okta.version>
        <spring-shell.version>2.0.0.RELEASE</spring-shell.version>
        <camel.version>2.24.1</camel.version>
        <nimbus-jose-jwt.version>6.0.2</nimbus-jose-jwt.version>

        <fastjson.version>1.2.58</fastjson.version>
        <druid.starter.version>1.1.18</druid.starter.version>
        <ttl.version>2.10.2</ttl.version>
        <mybatis-plus.version>3.3.1.tmp</mybatis-plus.version>
        <hutool.version>5.3.7</hutool.version>
        <apollo.version>1.4.0</apollo.version>
        <mongodb-plus.version>1.0.0.RELEASE</mongodb-plus.version>
        <aliyun-sdk-oss.version>3.5.0</aliyun-sdk-oss.version>
        <kaptcha.version>2.3.2</kaptcha.version>
        <jasypt.version>2.1.1</jasypt.version>
        <swagger.version>2.9.2</swagger.version>
        <swagger-bootstrap-ui.version>1.9.4</swagger-bootstrap-ui.version>
        <sharding-sphere.version>4.0.0-RC1</sharding-sphere.version>
        <tx-lcn.version>5.0.2.RELEASE</tx-lcn.version>
        <discovery.plugin.version>5.2.1</discovery.plugin.version>
        <dozer.version>6.5.0</dozer.version>
        <redisson.version>3.11.1</redisson.version>

        <spring-cloud-commons.version>2.1.2.RELEASE</spring-cloud-commons.version>
        <spring-cloud-netflix.version>2.1.2.RELEASE</spring-cloud-netflix.version>
        <spring-cloud-openfeign.version>2.1.2.RELEASE</spring-cloud-openfeign.version>
        <spring-cloud-bus.version>2.1.2.RELEASE</spring-cloud-bus.version>
        <spring-cloud-gateway.version>2.1.2.RELEASE</spring-cloud-gateway.version>


    </properties>

