    <repositories>
        <!--<repository>-->
        <!--<id>genomics</id>-->
        <!--<url>http://10.225.12.115:8881/repository/maven-public/</url>-->
        <!--<snapshots>-->
        <!--<enabled>true</enabled>-->
        <!--</snapshots>-->
        <!--<releases>-->
